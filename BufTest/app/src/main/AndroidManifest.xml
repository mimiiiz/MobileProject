<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.buftest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddMenuActivity" />
        <activity android:name=".MenuActivity" />
        <activity
            android:name=".ViewCodeActivity"
            android:label="@string/title_activity_view_code"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".GenCodeActivity"
            android:label="@string/title_activity_gen_code"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".OrderActivity" />
        <activity android:name=".OrderDetailActivity"></activity>
    </application>

</manifest>